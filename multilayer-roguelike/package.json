{
	"name": "multiplayer-roguelike",
	"version": "1.0.0",
	"description": "A typescript template project for Phaser 3 using Parceljs",
	"scripts": {
		"start": "parcel src/client/index.html -p 8000",
		"start-server": "ts-node-dev --project tsconfig.server.json src/server/index.ts",
		"build": "parcel build src/client/index.html --out-dir dist",
		"test": "echo \"Error: no test specified\" && exit 1",
		"lint": "eslint ./src --ext .js,.jsx,.ts,.tsx"
	},
	"devDependencies": {
		"@types/cors": "^2.8.6",
		"@types/express": "^4.17.1",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"buffer": "^5.7.1",
		"eslint": "^6.8.0",
		"minimist": ">=1.2.2",
		"parcel-plugin-clean-easy": "^1.0.2",
		"parcel-plugin-static-files-copy": "^2.4.3",
		"ts-node": "^8.1.0",
		"ts-node-dev": "^1.0.0-pre.63",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"@geckos.io/client": "^2.2.3",
		"@mikewesthad/dungeon": "^2.0.1",
		"axios": "^0.27.2",
		"client": "^0.0.1",
		"cors": "^2.8.5",
		"express": "^4.16.4",
		"parcel": "^2.8.2",
		"phaser": "^3.55.2",
		"tslib": "^2.4.0"
	},
	"parcelCleanPaths": [
		"dist"
	],
	"staticFiles": {
		"staticPath": "./Public",
		"watcherGlob": "**"
	}
}
