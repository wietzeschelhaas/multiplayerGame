{
  "name": "geckos-server",
  "version": "1.0.0",
  "description": "Geckos v2 uses ESM. Here is an example how to use it in a CJS project.",
  "main": "dist/index.js",
  "type": "commonjs",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run tsc && npm-run-all --parallel dev:*",
    "dev:tsc": "tsc --watch",
    "dev:nodemon": "nodemon dist/server.js",
    "tsc": "tsc --build"
  },
  "keywords": [],
  "dependencies": {
    "@geckos.io/server": "^2.1.4",
    "@mikewesthad/dungeon": "^2.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.0-dev.20211206"
  }
}
